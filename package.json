{
  "name": "uriel",
  "version": "1.6.0",
  "description": "A NodeJS statsd agent to push memory, cpu, network, swap, diskio, and disk usage over UDP",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "test": "nyc mocha",
    "build": "gulp",
    "fix": "gulp fix",
    "lint": "gulp lint",
    "coverage": "nyc --reporter=lcov mocha && cat ./coverage/lcov.info | codacy-coverage",
    "pretty": "gulp pretty",
    "config": "node ./scripts/configure.js",
    "deconfig": "node ./scripts/deconfigure.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MediaXPost/uriel.git"
  },
  "keywords": [
    "statsd-agent",
    "statsd",
    "agent",
    "node",
    "nodejs",
    "telegraf",
    "DogStatsD"
  ],
  "author": "Jay Reardon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MediaXPost/uriel/issues"
  },
  "homepage": "https://github.com/MediaXPost/uriel#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@mediaxpost/lodashext": "^1.0.0",
    "chalk": "^2.1.0",
    "change-case": "^3.0.1",
    "commander": "^2.11.0",
    "hot-shots": "^5.0.0",
    "logstub": "^1.0.0-2",
    "moment": "^2.19.3",
    "systeminformation": "^3.30.4",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "codacy-coverage": "^2.0.3",
    "eslint": "^4.10.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-mocha": "^4.3.1",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-prettier": "^1.1.0",
    "inquirer": "^4.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "prettier": "^1.8.2",
    "unit.js": "^2.0.0"
  }
}

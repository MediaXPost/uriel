{
  "name": "uriel",
  "version": "2.0.0",
  "description": "A NodeJS statsd agent to push memory, cpu, network, swap, diskio, and disk usage over UDP",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "test": "nyc mocha",
    "build": "gulp",
    "fix": "gulp fix",
    "lint": "gulp lint",
    "coverage": "nyc --reporter=lcov mocha && cat ./coverage/lcov.info | codacy-coverage",
    "pretty": "gulp pretty",
    "config": "node ./scripts/configure.js",
    "deconfig": "node ./scripts/deconfigure.js",
    "docker-build": "docker build -t uriel .",
    "docker-run": "docker run -d uriel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MediaXPost/uriel.git"
  },
  "keywords": [
    "statsd-agent",
    "statsd",
    "agent",
    "node",
    "nodejs",
    "telegraf",
    "DogStatsD"
  ],
  "author": "Jay Reardon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MediaXPost/uriel/issues"
  },
  "homepage": "https://github.com/MediaXPost/uriel#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@mediaxpost/lodashext": "^1.2.1",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "hot-shots": "^6.1.0",
    "inquirer": "^6.2.1",
    "logstub": "^1.2.2",
    "moment": "^2.23.0",
    "systeminformation": "^3.54.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-match": "^1.1.1",
    "codacy-coverage": "^3.4.0",
    "eslint": "^5.12.0",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-mocha": "^6.0.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-prettier": "^2.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3"
  }
}
